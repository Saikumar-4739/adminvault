  "scripts": {
    "clean": "powershell -ExecutionPolicy Bypass -File clean-workspace.ps1",
    "build:shared": "npx tsc --build packages/libs/shared-models/tsconfig.lib.json && npx tsc --build packages/libs/backend-utils/tsconfig.lib.json",
    "build:backend": "npm run build:shared && npx nx build backend --skip-nx-cache",
    "build:frontend": "npx nx build frontend",
    "build:all": "npm run build:shared && npm run build:backend && npm run build:frontend",
    "dev:backend": "npm run build:backend && cross-env TS_NODE_PROJECT=packages/backend/tsconfig.runtime.json nodemon --watch packages/backend/src --ext ts --exec \"npm run build:backend && node -r tsconfig-paths/register dist/packages/backend/main.js\"",
    "start:backend": "npm run build:backend && cross-env TS_NODE_PROJECT=packages/backend/tsconfig.runtime.json node -r tsconfig-paths/register dist/packages/backend/main.js",
    "dev:frontend": "npx nx dev frontend",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\""
  },
