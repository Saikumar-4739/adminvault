  "scripts": {
    "clean": "powershell -ExecutionPolicy Bypass -File clean-workspace.ps1",
    "build:shared": "npx tsc --build packages/libs/shared-models/tsconfig.lib.json && npx tsc --build packages/libs/backend-utils/tsconfig.lib.json",
    "build:backend": "npm run build:shared && npx nx build backend",
    "build:frontend": "npx nx build frontend",
    "build:all": "npm run build:shared && npm run build:backend && npm run build:frontend",
    "dev:backend": "npm run build:backend && nodemon --watch packages/backend/src --ext ts --exec \"npm run build:backend && node dist/packages/backend/main.js\"",
    "start:backend": "npm run build:backend && node dist/packages/backend/main.js",
    "dev:frontend": "npx nx dev frontend",
    "dev:mobile": "npx nx start mobile",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\""
  },
