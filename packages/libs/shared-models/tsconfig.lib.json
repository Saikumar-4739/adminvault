{
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "src",
    "outDir": "dist",
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2022",
    "lib": [
      "es2022"
    ],
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "composite": true,
    "tsBuildInfoFile": "dist/tsconfig.lib.tsbuildinfo",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "types": [
      "node"
    ],
    "paths": {
      "@adminvault/backend-utils": [
        "../backend-utils/src/index.ts"
      ]
    }
  },
  "include": [
    "src/**/*.ts"
  ],
  "references": []
}