{
  "compilerOptions": {
    "outDir": "../../dist/packages/backend",
    "types": [
      "node",
      "multer"
    ],
    "rootDir": "src",
    "tsBuildInfoFile": "../../dist/packages/backend/tsconfig.app.tsbuildinfo",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "target": "es2021",
    "strictPropertyInitialization": false,
    "noUnusedLocals": false,
    "composite": true,
    "declaration": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [
      "es2022"
    ],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "noImplicitAny": false,
    "importHelpers": true,
    "paths": {
      "@adminvault/shared-models": [
        "../libs/shared-models/src/index.ts"
      ],
      "@adminvault/backend-utils": [
        "../libs/backend-utils/src/index.ts"
      ],
      "@adminvault/shared-services": [
        "../libs/shared-services/src/index.ts"
      ]
    }
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [],
  "references": [
    {
      "path": "../libs/backend-utils/tsconfig.lib.json"
    },
    {
      "path": "../libs/shared-models/tsconfig.lib.json"
    }
  ]
}